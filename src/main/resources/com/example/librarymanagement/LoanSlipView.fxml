<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>



<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.librarymanagement.LoanSlipController">

    <HBox prefHeight="34.0" prefWidth="751.0">
        <Button onAction="#goToBookScene" prefWidth="80.0" text="Book">
            <HBox.margin>
                <Insets left="10.0" right="10.0" />
            </HBox.margin></Button>
        <Button onAction="#goToLoginScene" prefWidth="80.0" text="Log out">
            <HBox.margin>
                <Insets left="10.0" right="10.0" />
            </HBox.margin></Button>
    </HBox>

    <TableView fx:id="tableView">
        <columns>
            <TableColumn fx:id="idUserCol" text="User ID"/>
            <TableColumn fx:id="idBookCol" text="Book ID"/>
            <TableColumn fx:id="dateCol" text="Date"/>
            <TableColumn fx:id="returnDateCol" text="Return date"/>
            <TableColumn fx:id="statusCol" text="Status "/>
        </columns>
    </TableView>
</VBox>
