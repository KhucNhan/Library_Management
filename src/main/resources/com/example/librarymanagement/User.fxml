<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.librarymanagement.UserController"
            prefHeight="400.0" prefWidth="600.0">
    <TableView fx:id="tableUser" fixedCellSize="50.0" focusTraversable="false" prefHeight="670.0" prefWidth="605.0"
               style="-fx-border-color: white; -fx-background-color: white;">
        <columns>
            <TableColumn fx:id="userIdCol" minWidth="100.0"
                         style="-fx-border-color: lightgray; -fx-background-color: white;" text="ID"/>
            <TableColumn fx:id="usernameCol" minWidth="100.0"
                         style="-fx-background-color: white; -fx-border-color: lightgray;" text="Username"/>
            <TableColumn fx:id="passwordCol" minWidth="100.0"
                         style="-fx-border-color: lightgray; -fx-background-color: white;" text="Password"/>
            <TableColumn fx:id="roleCol" minWidth="100.0"
                         style="-fx-background-color: white; -fx-border-color: lightgray;" text="Role"/>
        </columns>
    </TableView>
</AnchorPane>
